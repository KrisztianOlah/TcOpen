    <Method Name="WaitForModulo" Id="{9c4f24df-3540-0661-3cbf-78190ae201fa}">
      <Declaration><![CDATA[METHOD WaitForModulo : BOOL
VAR_INPUT
	Modulo : INT;
END_VAR
]]></Declaration>
