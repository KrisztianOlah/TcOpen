    <Method Name="WaitForModulo" Id="{10f18902-db08-04f5-2555-668a13c2abef}">
      <Declaration><![CDATA[METHOD WaitForModulo : BOOL
VAR_INPUT
	Modulo : INT;
END_VAR
]]></Declaration>
