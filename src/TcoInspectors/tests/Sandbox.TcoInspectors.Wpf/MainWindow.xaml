<Window
    x:Class="Sandbox.TcoInspectors.Wpf.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Sandbox.TcoInspectors.Wpf"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
    xmlns:vortex="http://vortex.mts/xaml"
    Width="1000"
    Height="450"
    mc:Ignorable="d"
    Title="MainWindow">
    <Window.DataContext>
        <local:MainWindowViewModel />
    </Window.DataContext>
    <Grid Grid.Row="0">
        <vortex:TcoDigitalInspectorShadowControlView DataContext="{Binding TcoInspectorsPlc.MAIN._digitalInspectorTests._sut, Mode=OneWay}"/>
    </Grid>
</Window>
