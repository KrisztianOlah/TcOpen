<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TcOpen.Inxton.Data.Wpf" Version="0.5.0-alpha.822" />
    <PackageReference Include="TcOpen.Inxton.TcoCore.Wpf" Version="0.5.0-alpha.822" />
    <PackageReference Include="TcOpen.Inxton.TcoInspectors.Wpf" Version="0.5.0-alpha.822" />
    <PackageReference Include="TcOpen.Inxton.TcoPneumatics.Wpf" Version="0.5.0-alpha.822" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MainPlcConnector\MainPlcConnector.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\strings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>strings.resx</DependentUpon>
    </Compile>
    <Compile Update="Views\Libs\TcoTaskedSequencer\Sequence\TcoTaskedSequencerSequenceView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Views\Libs\TcoTaskedService\Sequence\TcoTaskedServiceComponentsView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Views\Libs\TcoTaskedService\Task\TcoTaskedServiceTaskView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\strings.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>strings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Views\Libs\TcoTaskedSequencer\Sequence\TcoTaskedSequencerSequenceView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Views\Libs\TcoTaskedService\Sequence\TcoTaskedServiceComponentsView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Views\Libs\TcoTaskedService\Task\TcoTaskedServiceTaskView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
